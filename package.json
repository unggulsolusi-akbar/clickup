  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@inquirer/input": "^4.2.5",
    "@inquirer/prompts": "^7.9.0",
    "commander": "^14.0.1",
    "moment": "^2.30.1",
    "puppeteer": "^24.24.1"
  }
